              <code_block> private async System.Threading.Tasks.Task AuthenticateAsync()
 {
     while (user == null)
     {
         string message;
         try
         {
             // Change 'MobileService' to the name of your MobileServiceClient instance.
             // Sign-in using Facebook authentication.
             user = await App.MobileService
                 .LoginAsync(MobileServiceAuthenticationProvider.WindowsAzureActiveDirectory);

             UserInfo userInfo = await App.MobileService
                 .InvokeApiAsync&lt;UserInfo&gt;("getuserinfo", HttpMethod.Get, null);

             message = string.Format("User info for the logged in user...\n\n{0}",userInfo.ToString());
         }
         catch (InvalidOperationException)
         {
             message = "You must log in. Login Required";
         }

         var dialog = new MessageDialog(message);
         dialog.Commands.Add(new UICommand("OK"));
         await dialog.ShowAsync();
     }
 }

</code_block>
