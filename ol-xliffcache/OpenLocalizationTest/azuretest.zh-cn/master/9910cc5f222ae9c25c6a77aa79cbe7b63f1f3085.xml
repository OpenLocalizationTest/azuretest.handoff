              trans_unit_id="d036e66e-6d04-4d26-ad01-453bab915e42">
              <code_block> // Define a member variable for storing the signed-in user.
 var userId = null;

 // Request authentication from Mobile Services using a Facebook login.
 var authenticate = function () {
     return new WinJS.Promise(function (complete) {
         // Change 'client' to the name of your MobileServiceClient instance.
         // Sign-in using Facebook authentication.
         client.login("facebook").done(function (results) {
             userId = results.userId;
             refreshTodoItems();
             buttonLogin.disabled = true;
             var message = "You are now signed in as: " + userId;
             var dialog = new Windows.UI.Popups.MessageDialog(message);
             dialog.showAsync().done(complete);
         }, function (error) {
             userId = null;
             var dialog = new Windows.UI.Popups
             .MessageDialog(error);
                 //.MessageDialog("An error occurred during login", "Login Required");
             dialog.showAsync().done(complete);
         });
     });
 };

 // Handle the sign in button click event.
 buttonLogin.addEventListener("click", function () {
     authenticate();
 });

</code_block>
