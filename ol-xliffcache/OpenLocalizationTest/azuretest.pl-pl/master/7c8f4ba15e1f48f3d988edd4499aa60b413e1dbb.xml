      <MarkdownBlockBase
        xsi:type="MarkdownCodeBlock"
        item_id="e1588708-d84d-4025-8e28-57b65a40c427"
        item_type="CodeBlock"
        item_footer="footer_not_set"
        trans_unit_id="e1588708-d84d-4025-8e28-57b65a40c427">
        <code_block>static IAsset GetAsset(string assetId)
{
    // Use a LINQ Select query to get an asset.
    var assetInstance =
        from a in _context.Assets
        where a.Id == assetId
        select a;
    // Reference the asset as an IAsset.
    IAsset asset = assetInstance.FirstOrDefault();

    return asset;
}

</code_block>
