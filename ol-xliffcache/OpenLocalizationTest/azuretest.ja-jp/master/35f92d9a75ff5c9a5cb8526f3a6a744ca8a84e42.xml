            <MarkdownCodeBlock
              item_id="c453a17d-a57c-46bb-a778-fd9f5257d628"
              item_type="CodeBlock"
              item_footer="footer_not_set"
              trans_unit_id="c453a17d-a57c-46bb-a778-fd9f5257d628">
              <code_block> exports.register = function (registration, registrationContext, done) {   
     // Get the ID of the logged-in user.
     var userId = registrationContext.user.userId;    

     // Perform a check here for any disallowed tags.
     if (!validateTags(registration))
     {
         // Return a service error when the client tries 
         // to set a user ID tag, which is not allowed.      
         done("You cannot supply a tag that is a user ID");      
     }
     else{
         // Add a new tag that is the user ID.
         registration.tags.push(userId);

         // Complete the callback as normal.
         done();
     }
 };

 function validateTags(registration){
     for(var i = 0; i &lt; registration.tags.length; i++) { 
         console.log(registration.tags[i]);           
         if (registration.tags[i]
         .search(/facebook:|twitter:|google:|microsoft:/i) !== -1){
             return false;
         }
         return true;
     }
 }

</code_block>
